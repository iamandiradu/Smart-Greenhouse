// String Extension
String.prototype.escapeHtml = function() {
    var map = {
        "&": "&amp;",
        "'": "&#39;",
        '"': "&quot;",
        "<": "&lt;",
        ">": "&gt;"
    };

    return this.replace(/[&"'\<\>]/g, function (c) {
        return map[c];
    });
};

